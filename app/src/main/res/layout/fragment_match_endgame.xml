<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:padding="@dimen/activity_vertical_margin">

    <frc3824.rohawkticsscouting2017.Views.SavableSpinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/endgame_climb"
        app:key="endgame_climb"
        app:label="Climb"
        app:spinner_values="@array/endgame_climb_options"/>

    <frc3824.rohawkticsscouting2017.Views.SavableRadioButtons
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/endgame_climb_time"
        app:key="endgame_climb_time"
        app:label="Climb Time"
        app:radio_values="@array/endgame_climb_time_options"/>

</LinearLayout>