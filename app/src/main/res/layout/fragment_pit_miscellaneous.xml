<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/activity_vertical_margin">

    <frc3824.rohawkticsscouting2017.Views.SavableAutoCompleteTextView
        android:id="@+id/programming_language"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:autocomplete_values="@array/programming_language_options"
        app:key="programming_language"
        app:label="Programming Language" />
    <frc3824.rohawkticsscouting2017.Views.SavableAutoCompleteTextView
        android:id="@+id/drive_train"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:autocomplete_values="@array/drive_train_options"
        app:key="drive_train"
        app:label="Drive Train" />
    <frc3824.rohawkticsscouting2017.Views.SavableCounter
        android:id="@+id/cims"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:key="cims"
        app:label="CIMS" />
    <frc3824.rohawkticsscouting2017.Views.SavableNumeric
        android:id="@+id/max_hopper_load"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:key="max_hopper_load"
        app:label="Maximum Hopper Load" />

</LinearLayout>